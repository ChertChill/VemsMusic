@model MusicalGroup

<div class="container">
    <div class="row align-items-center mb-5">

        <h2 class="text-center mt-3 mb-3">@Model.Name</h2>

        @foreach (Music item in Model.Musics)
        {
            @await Html.PartialAsync("_OneMusicOnScreen", item)
        }

    </div>
</div>