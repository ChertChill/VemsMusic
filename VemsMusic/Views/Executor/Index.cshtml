@model GroupWithMusicsViewModel

<div class="container">
    <div class="row">

        <h2 class="text-center mt-3 mb-3">@Model.MusicalGroup.Name</h2>

        @foreach (Music item in Model.GetAllMusic)
        {
            @await Html.PartialAsync("OneMusicOnScreen", item)
        }

    </div>
</div>